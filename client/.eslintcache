en w-screen flex flex-col items-center content-center justify-center font-bold font-m-plus-rounded text-white\">\r\n      <Link to={{ pathname: \"/\" }} className=\"absolute top-5 left-5\">\r\n        <i className=\"material-icons back-button\">arrow_back</i>\r\n      </Link>\r\n      <h3 className=\"lg:text-6xl text-4xl font-bold font-m-plus-rounded pb-1 \">\r\n        Log In\r\n      </h3>\r\n      {errors.user ? <p>{errors.user}</p> : \"\"}\r\n      {errors.password ? <p>{errors.password}</p> : \"\"}\r\n      <form\r\n        onSubmit={(e) => onSubmit(e)}\r\n        className=\"flex flex-col justify-between items-center w-2/3 mt-20 lg:w-1/2\"\r\n      >\r\n        {/* Email */}\r\n        <label htmlFor=\"email\" className=\"self-start\">\r\n          E-mail\r\n        </label>\r\n        <input\r\n          type=\"email\"\r\n          id=\"email\"\r\n          type=\"email\"\r\n          className=\"mb-5 p-1 w-full rounded-md text-gray-700\"\r\n          name=\"email\"\r\n          placeholder=\"Email\"\r\n          value={formData.email}\r\n          onChange={(e) => onChange(e)}\r\n        />\r\n\r\n        {/* Password */}\r\n        <label htmlFor=\"password\" className=\"self-start\">\r\n          Password\r\n        </label>\r\n        <input\r\n          type=\"password\"\r\n          id=\"password\"\r\n          type=\"password\"\r\n          className=\"mb-5 p-1 w-full rounded-md text-gray-700\"\r\n          name=\"password\"\r\n          placeholder=\"password\"\r\n          value={formData.password}\r\n          onChange={(e) => onChange(e)}\r\n        />\r\n\r\n        <button\r\n          type=\"submit\"\r\n          name=\"action\"\r\n          className=\"bg-blue-700 lg:w-40 lg:mx-36 text-lg rounded-lg p-2 text-white w-2/3 font-bold font-m-plus-rounded\"\r\n        >\r\n          LOG IN\r\n        </button>\r\n      </form>\r\n      <div className=\"mt-10 lg:w-1/2 \">\r\n        <p className=\"mb-2\">Demo Login:</p>\r\n        <p>Email: code7sandb